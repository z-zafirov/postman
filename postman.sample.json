{
	"info": {
		"_postman_id": "acc13029-0e1e-4b0f-aa71-e4b11ca0ce9b",
		"name": "Postman",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "sample",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let responseData = pm.response.json();",
							"let token = responseData.headers[\"postman-token\"];",
							"",
							"pm.test(\"Positive: HTTP status 200\", () =>{",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Negative: HTTP status 500\", () =>{",
							"    pm.response.to.have.status(500);",
							"});",
							"",
							"pm.test(\"Positive: Postman token not null\", () =>{ ",
							"    pm.expect(token).to.be.not.null;",
							"});",
							"",
							"pm.test(\"Negative: Accept encoding is null\", () =>{",
							"    let token = responseData.headers[\"accept-encoding\"];",
							"    pm.expect(token).to.be.null;",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "https://postman-echo.com/get",
					"protocol": "https",
					"host": [
						"postman-echo",
						"com"
					],
					"path": [
						"get"
					]
				}
			},
			"response": []
		}
	]
}